algoritmo "CONTAGEM INTELIGENTE"

var
   N1, N2, contador: inteiro
inicio
      Escreval ("----------------------")
      Escreval ("CONTAGEM INTELIGENTE")
      Escreva  ("IN√çCIO :")
      Leia (N1)
      Escreva  ("FIM :")
      Leia (N2)
      se N1 < N2 entao
         contador <- N1
         Enquanto (contador <= N2) faca
         escreva (contador, "...")
         contador <- contador + 1
         Fimenquanto
      senao
         se N1 > N2 entao
            contador <- N1
            Enquanto (contador >= N2) faca
            escreva (contador, "...")
            contador <- contador - 1
            Fimenquanto
         Fimse
      Fimse
fimalgoritmo
